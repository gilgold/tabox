{
  "name": "tabox",
  "version": "3.5.1",
  "description": "Tabox - Save and share your tabs!",
  "main": "index.js",
  "scripts": {
    "test": "jest --transformIgnorePatterns \"node_modules/(?!@toolz/allow-react)/\" --env=jsdom",
    "dev": "INLINE_RUNTIME_CHUNK=false NODE_ENV=development webpack --mode development --config webpack.js --watch",
    "prod": "INLINE_RUNTIME_CHUNK=false NODE_ENV=production webpack --mode production --config webpack.js",
    "lint": "eslint ./ --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gilgold/tabox.git"
  },
  "keywords": [
    "tabox",
    "tab",
    "groups"
  ],
  "author": "Gil Goldstein",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gilgold/tabox/issues"
  },
  "homepage": "https://tabox.co",
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "setupFiles": [
      "jest-webextension-mock",
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@sentry/react": "^6.14.3",
    "@sentry/tracing": "^6.14.3",
    "@szhsin/react-menu": "^2.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chrome-extension-async": "^3.4.1",
    "javascript-time-ago": "^2.3.8",
    "react": "^17.0.2",
    "react-confirm-alert": "^2.7.0",
    "react-countdown-circle-timer": "^2.5.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-simple-animate": "^3.3.12",
    "react-simple-snackbar": "^1.1.11",
    "react-sortablejs": "^6.0.0",
    "react-time-ago": "^7.0.0",
    "react-tiny-popover": "^7.0.1",
    "react-tooltip": "^4.2.21",
    "react-uid": "^2.3.1",
    "recoil": "^0.5.2",
    "sortablejs": "^1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/jest": "^27.0.2",
    "@types/sortablejs": "^1.10.7",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.2.0",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-react": "^7.27.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "jest-webextension-mock": "^3.7.19",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.2.1",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-extension-manifest-plugin": "^0.8.0"
  }
}
